{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "concurrently \"npm run watch-sass-js\" \"npm run start-js\"",
    "build": "npm run eslint-build && npm run stylelint-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint-dev": "eslint --fix src/**/*.js --cache --cache-location .eslintcache",
    "eslint-build": "eslint --fix src/**/*.js",
    "stylelint-dev": "stylelint --fix src/**/*.scss --cache --cache-location .stylelintcache",
    "stylelint-build": "stylelint --fix src/**/*.scss",
    "watch-sass-js": "npm-watch"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint-dev": {
      "patterns": [
        "src/**/*.js"
      ],
      "extensions": "js"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run build",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "node-sass": "^4.13.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.19.1",
    "standart": "^6.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1"
  }
}
